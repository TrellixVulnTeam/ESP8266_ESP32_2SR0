  "version": "0.2.0",
  "configurations": [

    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/ftdinandreader",
      //"args": ["-f CP1416RAUGG-rawstorage.bin", "-s 128", "-w 64"],
      "args": ["-r kernel-OOB.bin", "-p 0x2000000 0x22A22F0"],      
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      //"cwd": "/home/name/nandreader/nandread_FTDI232H",      
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    },
  ]
}