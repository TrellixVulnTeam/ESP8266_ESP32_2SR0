  "version": "0.2.0",
  "configurations": [

    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bitbang_ft232",
      //"args": ["-s 258", "-r 1"],
      //"args": ["-b 0"],
      //"args": ["-f bank_2.bin", "-s 16384", "-w 15000"],
      //"args": ["-f b.bin", "-s 1", "-r 19"],
      "args": ["-f CP1416RAUGG-rawstorage.bin", "-s 128", "-w 64"],
      
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      //"cwd": "/home/name/nandreader/nandread_FTDI232H",      
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    },
  ]
}