  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/ftdinandreader",
      //"args": ["-i"],
      //"args": ["-r", "kernel-OOB.bin", "-p", "0x2000000", "0x22A22F0"],
      //"args": ["-e", "-p", "0x0", "0x63000"],
      //"args": ["-w", "kernel/kernel_test.bin", "-p", "0x0", "0x63000"],
      //"args": ["-r", "kernel_test_read", "-p", "0x2000000", "0x2100000"],
      //"args": ["-r", "kernel_test_read", "-p", "0x0", "0x63000"],
      "args": ["-r", "kernel_test_read", "-p", "0x0", "0x60000"],
      //"args": ["-w", "kernel/kernel_test.bin", "-p", "0x01FF8000", "0x2100000"],
      
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "sourceFileMap": {
        "ftdinandreader": "~/nandreader/libftdi1-1.5/src",
      }      
      //"sourceMapPathOverrides": {
      //  "/source/*": "${workspaceFolder}/*"
      //}
    }
  ]
}